{
  "expo": {
    "name": "Balance Seekr",
    "slug": "balance-seekr",
    "version": "1.0.0",
    "scheme": "balanceseekr",
    "orientation": "portrait",
    "extra": {
      "apiUrl": "https://balance-seekr-production.up.railway.app"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.balanceseekr.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "package": "com.balanceseekr.app",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "balanceseekr"
            }
          ]
        }
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-sqlite",
      "expo-secure-store",
      "expo-audio",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you set background images for your Mastermind groups."
        }
      ]
    ]
  }
}
